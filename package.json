{
  "name": "vue3-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .ts,vue src/** --no-error-on-unmatched-pattern --quiet",
    "lint:fix": "eslint --ext .ts,vue src/** --no-error-on-unmatched-pattern --quiet --fix"
  },
  "dependencies": {
    "@vueuse/core": "^6.7.4",
    "koa": "^2.13.4",
    "mockjs": "^1.1.0",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "autoprefixer": "^10.3.7",
    "babel-jest": "^26.6.3",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^11.2.6",
    "typescript": "4.1.5",
    "vite": "^2.6.4",
    "vite-plugin-mock": "^2.9.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  }
}
